<% include ./partials/header %>
<div class="container ">


  
<table class="table table-striped table-bordered mydatatable student-table" style="width: 100%">
<thead >
  <tr>
    <th scope="col">Regn No.</th>
    <th scope="col">Name</th>
    <th scope="col">Branch</th>
    <th scope="col">DMC Taken</th>
    <th scope="col">Updated On</th>
    <th scope="col">Actions</th>
  </tr>
</thead>
<tbody>
<% students.forEach(function(student){ %>

  <tr>
    <th scope="row"><%= student.regn_no %></th>
    <td><%= student.name %></td>
    <td><%= student.branch %></td>
    <td><%= student.DMC %></td>
    <td><%= student.created.toDateString() %></td>
  
    <td>
    <ul id="action">
    <li>
    <a href="/students/<%=student._id %>" class="fas fa-eye"></a >
    </li>

    <li>
    <a href="/students/<%=student._id %>/edit" class="fas fa-user-edit"></a >  
    </li>

    <li>
    <form id="delete" action="/students/<%= student._id%>?_method=DELETE" method="POST">
    <button class="fas fa-trash-alt"></button></form>
    
    
    </li>

    </ul>
    </td>
  </tr>
  <%})%>

</tbody>
</table>


</div>
       

<% include ./partials/footer %>
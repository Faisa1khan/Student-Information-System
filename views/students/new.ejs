<% include ../partials/header %>

<div class="">

<form action="/students" method="POST">

<div class="form-row">
<div class="form-group col-md-6">
<label for"regn_no">Registration No.</label>
<input type="text"  id="regn_no" class="form-control " name="student[regn_no]" placeholder="regn_no" >
</div>

<div class="form-group col-md-6">
<label for"name">Name</label>
<input type="text"  id="name" class="form-control " name="student[name]" placeholder="name" >
</div>

<div class="form-group col-md-6">
<label for="branch">Branch</label>
        <select id="branch" class="form-control" name="student[branch]">
          <option selected>Choose Branch</option>
          <option>CSE</option>
          <option>CE</option>
          <option>ME</option>
          <option>ECE</option>
          <option>EEE</option>
        </select>
</div>

<div class="form-group col-md-6">
<label for"dmc">DMC Taken</label>

<select id="dmc" class="form-control" name="student[DMC]">
    <option selected> Yes OR No ?</option>
    <option>YES</option>
    <option>NO</option>
  </select>
</div>
</div>

<!-- <div class="form-group">
<label for"image">Image</label>
<input type="text"  id="image" class="form-control " name="student[image]" placeholder="image Url" >
</div> -->



<button type="submit" class="btn btn-primary">Submit</button>


</form>
</div>

